{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "phone": {
      "type": "string",
      "maxLength": 12,
      "required": true,
      "unique": true
    },
    "pincode": {
      "type": "string",
      "required": false,
      "maxLength": 4
    },
    "resetPasswordPincode": {
      "type": "string",
      "maxLength": 4,
      "minLength": 4
    },
    "tickets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ticket.ticket",
      "mappedBy": "users_permissions_user"
    },
    "favorite_places": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::place.place"
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "trip_places": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::place.place"
    },
    "home": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::place.place"
    },
    "support_services": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::support-service.support-service",
      "mappedBy": "user"
    },
    "date_birth": {
      "type": "date"
    },
    "isVisuallyImpaired": {
      "type": "boolean"
    },
    "isHearingImpaired": {
      "type": "boolean"
    },
    "isRestrictedInMovement": {
      "type": "boolean"
    },
    "ready_routes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ready-route.ready-route"
    }
  }
}
